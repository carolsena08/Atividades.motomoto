<body>

    <script>
    let idade = prompt("Digite a sua idade");
    if(idade == ""){
        document.writeln("Digite um valor para continuar");
        document.writeln("Pressione F5");
    }else{
    if(idade >=18){
        alert("Você é maior de idade");
    }else{
        alert("Você é menor de idade");
    }
}
    
</script>

</body>
